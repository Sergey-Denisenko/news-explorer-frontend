{
  "extends": "airbnb-base",
  "env": {
    "es6": true
  },
  "rules": {
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "react/jsx-uses-vars": "error",
    "react/jsx-uses-react": "error"
  },
  "parser": "babel-eslint",
  "plugins": ["react"]
}
